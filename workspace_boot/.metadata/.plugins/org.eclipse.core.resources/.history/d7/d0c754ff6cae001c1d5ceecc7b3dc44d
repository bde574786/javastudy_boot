package ch14;

public class Engin {

	//포함 관계
	Engin engin;
	String name;
	int price;
	
	public Engin getEngin() {
		return engin;
	}
	public void setEngin(Engin engin) {
		this.engin = engin;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	
	
	
	
	// 도전 과제
	// 각각의 클래스를 메모리에 올려서 값을 넣고
	// 화면에 결과를 출력하시오 !!!
	
	
}
