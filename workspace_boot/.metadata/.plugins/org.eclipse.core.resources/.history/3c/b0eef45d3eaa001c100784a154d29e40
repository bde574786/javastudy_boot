package ch04;

public class ContinueTest {

	public static void main(String[] args) {

		// 예약어 continue
		// 무시하고 진행하는 continue
		final int MULTIPLE = 3;
		final int MAX = 10;

		int num = 0;
		int count = 0;

		for (num = 1; num <= MAX; num++) {

			// 3에 배수이면 화면에 출력하지 마세요 !!!

			if (num % MULTIPLE == 0) {
				continue; // <-- 무시하고 진행하라 !!
			}
			System.out.println("num : " + num);

		}
	}

}
