package ch06;

public class Bus {

	// 상태 (버스 번호, 승객 수, 수입금)
	int busNumber;
	int passengerCount;
	int money;
	
	// 행위
	public void take(int money) {
		this.money += money;
	}
	
	// 상태 창
	public void shoeInfo() {
		System.out.println("버스 번호 : " + busNumber);
		System.out.println("승객 수 : " + passengerCount);
		System.out.println("금액 : " + money);
	}
	
	
	
}
