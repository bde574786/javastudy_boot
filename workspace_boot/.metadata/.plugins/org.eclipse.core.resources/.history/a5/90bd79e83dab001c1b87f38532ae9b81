package ch08;

public class UserInfo {

	//  멤버 변수
	// this 활용한 생성
	
	String name;
	String address;
	String phoneNumber;
	
	
	public UserInfo(){
	}
	
	public UserInfo(String name, String address) {
		this.name = name;
		this.address = address;
	}
	
	public UserInfo(String name, String address, String phoneNumber){
		this(name, address);
		this.phoneNumber = phoneNumber;
	}
	
	public void showInfo() {
		System.out.println("이름 : "+name+"\n주소 : "+ address + "전화번호 : "+ phoneNumber);
	}
	
	
	
	public static void main(String[] args) {
		
		
		UserInfo user = new UserInfo("efd","awefaew","awef");
		user.showInfo();
		
	
		
		
		
		
		// 여기서 메인 함수 사용할 수 있다.
		
		
	}
	
	
}
