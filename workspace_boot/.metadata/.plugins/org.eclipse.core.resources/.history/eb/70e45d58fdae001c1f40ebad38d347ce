package starcraft_game.zerg;

public class Hatchery {

	private Zergling makeZergling() {
		System.out.println("히드라를 생산합니다.");
		return new Zergling();
	}
	
	private Hydra makeHydra() {
		System.out.println("히드라를 생산합니다.");
		return new Hydra();
	}
	
	private Ultra makeUltra() {
		System.out.println("히드라를 생산합니다.");
		return new Ultra();
	}
	
}
