	}

	public void setGrade() {	
		
	}

	public double getWeight() {
		return weight;
	}
	
	public void setWeight() {
		
	}
	
	public boolean getIsMarried() {
		return isMarried;
	}
	
	public void setIsMarried() {
		
	}
}
	
	
