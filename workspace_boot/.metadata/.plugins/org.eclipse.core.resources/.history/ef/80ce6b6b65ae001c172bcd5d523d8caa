package ch15;

public class MainTest3 {

	public static void main(String[] args) {

			Banana banana1 = new Banana();
			Banana banana2 = new Banana();
		
			// 배열
			// 주의점 : 배열을 사용할 때는 배열의 크기를 먼저 지정해야 한다.
			Banana[] bananas = new Banana[2];
			bananas[0] = banana1;
			bananas[1] = banana2;
	}

}
