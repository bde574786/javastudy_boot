package ch13_1;

public class MainTest {

	public static void main(String[] args) {

		Zealot zealot = new Zealot("질럿", 10, 5);
		Marine marine = new Marine("마린", 10, 5);
		Zergling zergling = new Zergling("저글링", 10, 5);
		
		zealot.attack(marine);
		marine.attack(zealot);
		zergling.attack(marine);
		zealot.showInfo();
	}

}
