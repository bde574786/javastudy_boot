package ch14;

public class Hero {

	private String name;
	private int hp;
	
	Hero(String name, int hp){
		this.name = name;
		this.hp = hp;
	}
	
	public void attack() {
		System.out.println(this.name + "이 공격 중");
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getHp() {
		return hp;
	}

	public void setHp(int hp) {
		this.hp = hp;
	}
	
	
}
